    hexactpos 0
    vexactpos 0
    hexactsize 0
    vexactsize 0
    {
        PanelWidgetClass MainPanel {
            visible 1
            color 0.05 0.05 0.05 1
            position 0 0
            size 600 400
            halign center_ref
            valign center_ref
            hexactpos 1
            vexactpos 1
            hexactsize 1
            vexactsize 1
            style DayZDefaultPanel
            {
                TextListboxWidgetClass SongList {
                    visible 1
                    position 10 10
                    size 280 340
                    hexactpos 1
                    vexactpos 1
                    hexactsize 1
                    vexactsize 1
                    style Default
                }
                TextListboxWidgetClass PlayerList {
                    visible 1
                    position 310 10
                    size 280 340
                    hexactpos 1
                    vexactpos 1
                    hexactsize 1
                    vexactsize 1
                    style Default
                }
                ButtonWidgetClass PlayButton {
                    visible 1
                    position 10 360
                    size 580 30
                    hexactpos 1
                    vexactpos 1
                    hexactsize 1
